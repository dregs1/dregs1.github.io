				<!--changelog content-->
				<control type="textbox" id="5">
					<left>80</left>
					<top>169</top>
					<bottom>15</bottom>
					<width>1045</width>
					<label></label>
					<font>label</font>
					<align>justify</align>
					<pagecontrol>61</pagecontrol>
				</control>
				<!--scrollbar-->
				<control type="scrollbar" id="61">
					<left>48</left>
					<top>175</top>
					<height>860</height>
					<onleft>61</onleft>
					<onright>61</onright>
					<ondown>61</ondown>
					<onup>61</onup>
